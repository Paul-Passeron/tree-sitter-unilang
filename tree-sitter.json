{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "unilang",
      "camelcase": "Unilang",
      "title": "Unilang",
      "scope": "source.unilang",
      "file-types": ["ul"],
      "injection-regex": "^unilang$",
      "class-name": "TreeSitterUnilang"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for the Unilang programming language",
    "authors": [
      {
        "name": "Paul Passeron",
        "email": "paul.passeron2@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/Paul-Passeron/tree-sitter-unilang"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
